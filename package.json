    "scripts": {
        "start": "react-app-rewired start",
        "build": "GENERATE_SOURCEMAP=false react-scripts build",
        "ipfs": "rm -rf build && react-scripts build && CID=$(ipfs add -r build | tail -1 | cut -d' ' -f2) && echo '\n' && echo $CID",
        "ipfs-push": "CID=$(ipfs add -r build | tail -1 | cut -d' ' -f2) && echo '\n' && echo $CID && ipfs pin remote add --service=Pinata --name=MelalieGraph_$npm_package_version $CID",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "ghp": "react-scripts build && gh-pages -d build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
